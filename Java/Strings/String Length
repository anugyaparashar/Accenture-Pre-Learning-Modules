
import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        //Fill the code here
        Scanner sc=new Scanner(System.in); //Initializing Scanner object 
        String str=sc.nextLine();//Taking Input String
        int len=str.length();
        if(len%2==0) //finding if length is even or not
        System.out.println(len+" Even");
        else
        System.out.println(len+" Odd");
    }
}